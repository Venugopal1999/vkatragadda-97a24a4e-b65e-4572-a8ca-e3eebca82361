{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2021",
    "outDir": "../dist/api/seed",
    "rootDir": "..",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  },
  "include": [
    "src/database/seed.ts",
    "src/database/seed-bootstrap.js",
    "src/entities/**/*.ts",
    "../auth/src/**/*.ts"
  ]
}
